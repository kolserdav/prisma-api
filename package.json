{
  "name": "prisma-api",
  "version": "1.0.15",
  "main": "dist/src/index.js",
  "repository": "https://github.com/prisma-api/prisma-api",
  "maintainers": [
    "Sergey Kolmiller <uyem.ru@gmail.com>"
  ],
  "license": "MIT",
  "prisma": {
    "schema": "src/core/orm/schema.prisma"
  },
  "bin": {
    "prisma-api": "./dist/src/bin/prisma-api.js"
  },
  "types": "index.d.ts",
  "engines": {
    "node": "16.x"
  },
  "files": [
    "README.md",
    "dist",
    "docs",
    "index.d.ts",
    "src",
    "tsconfig.json"
  ],
  "scripts": {
    "start": "node ${npm_package_main}",
    "watch": "tsc -p . -w",
    "bins": "NPM_PACKAGE_VERSION=${npm_package_version} ./dist/src/bin/prisma-api.js",
    "dev:dev": "NODE_ENV=development nodemon ${npm_package_main}",
    "start:win": "set NODE_ENV=development && nodemon ${npm_package_main}",
    "dev:build": "yarn generate && tsc -p . && yarn script env",
    "migrate:dev": "yarn format && yarn generate && prisma migrate dev --preview-feature",
    "migrate": "prisma migrate deploy --preview-feature && yarn generate",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:status": "prisma migrate status --preview-feature",
    "studio": "prisma studio",
    "generate": "prisma generate",
    "format": "prisma format",
    "script": "node dist/src/scripts/index.js",
    "headers": "NODE_ENV=production file-headers"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-headers": "^2.0.2",
    "prettier": "^2.4.1",
    "yarn": "^1.22.16"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chokidar": "^2.1.3",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/gulp": "^4.0.9",
    "@types/html-to-text": "^8.0.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/nodemailer": "^6.4.4",
    "@types/node": "^16.10.3",
    "prisma": "^3.2.1",
    "nodemon": "^2.0.13",
    "typescript": "^4.4.3",
    "@prisma/client": "^3.2.1",
    "bcrypt": "^5.0.1",
    "chokidar": "^3.5.2",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "html-to-text": "^8.0.0",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.7.0"
  }
}
